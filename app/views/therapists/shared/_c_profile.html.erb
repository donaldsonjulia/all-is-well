<!-- if user is client, edit should be included as a class in outer section -->
<section class="c-data data">

  <div class="data-block name"><h3>Client&nbsp;Name: </h3><span><%= @client_name %></span></div>
  <div class="data-block dob"><h3>Date&nbsp;of&nbsp;Birth: </h3><span><%= @client_dob.strftime("%m/%d/%Y") %></span></div>
  <div class="data-block phone"><h3>Primary&nbsp;Phone&nbsp;Number: </h3><span><%= @client_phone %></span></div>
  <div class="data-block em_phone"><h3>Emergency&nbsp;Phone&nbsp;Number: </h3><span><%= @client_contact %></span></div>
  <div class="data-block gen_pract"><h3>General&nbsp;Practicioner: </h3><span><%= @current_client.general_prac %></span></div>
  <div class="data-block gender"><h3>Gender: </h3><span><%= @current_client.gender %> </span></div>
  <div class="data-block meds"><h3>Current&nbsp;Medications: </h3><span><%= @current_client.current_meds %></span
  <div class="med-list">
    </div>
  </div>

</section>
