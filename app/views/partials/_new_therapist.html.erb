<%= form_for :therapist, url: new_therapist_path, method: :post do |f| %>

  <%= f.label :first_name %><br/>
  <%= f.text_field :first_name %><br/>

  <%= f.label :last_name %><br/>
  <%= f.text_field :last_name %><br/>

  <%= f.label :cred %><br/>
  <%= f.text_field :cred %><br/>

  <%= f.label :phone %><br/>
  <%= f.text_field :phone %><br/>

  <%= f.label :email %><br/>
  <%= f.text_field :email %><br/>

  <%= f.submit "New Therapist Account" %>
<% end %>
